package Assignments.Problem7;

public class Problem7 {

    public int returnNumber(){

        int c = 0;

        float a = 7.8f;

        float b = 4.4f;


        c= (int) (a+b);

        return c;

    }

    public static void main(String[] args) {
        Problem7 p7 = new Problem7();
        System.out.println("value of C:" + p7.returnNumber());
    }
}
