package Assignments;

public class Problem1 {


    public void addFloatNumbers(float num1,float num2)
    {
        float num3 = num1+num2;
        System.out.println("Addition of two float numbers:"+num3);
    }
    public static void main(String[] args) {

        Problem1 p = new Problem1();
        p.addFloatNumbers(10.5f,20.5f);

    }
}
